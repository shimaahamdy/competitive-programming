#include <iostream>
using namespace std;

int main() {
	int x=0,step;
	    for(int i=1;i<=5;i++)
	    {
	        for(int j=1;j<=5;j++)
	        {
	            cin>>x;
	            if(x==1)
	            {
	               step=abs(2-i)+abs(2-j);

	        }
	        }
	    }


	    cout<<step;
	return 0;
}
